{
  "name": "danfojs",
  "version": "1.1.2",
  "description": "JavaScript library providing high performance, intuitive, and easy to use data structures for manipulating and processing structured data.",
  "main": "dist/danfojs-browser/src/index.js",
  "types": "dist/danfojs-browser/src/index.d.ts",
  "module": "lib/bundle-esm.js",
  "directories": {
    "test": "tests"
  },
  "contributors": [
    {
      "name": "Rising Odegua"
    },
    {
      "name": "Stephen Oni"
    }
  ],
  "files": [
    "lib/",
    "dist/"
  ],
  "dependencies": {
    "@tensorflow/tfjs": "^4.5.0",
    "mathjs": "11.8.0",
    "papaparse": "^5.4.1",
    "plotly.js-dist-min": "2.22.0",
    "table": "6.8.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz"
  },
  "scripts": {
    "test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:clean": "yarn build:clean && yarn run test",
    "build": "node ./scripts/prebuild.js && tsc && yarn run build:es5-bundle && yarn run build:esm-bundle",
    "build:es5-bundle": "webpack --mode production",
    "build:esm-bundle": "esbuild src/index.ts --bundle --format=esm --platform=browser --target=esnext --minify --legal-comments=none --outfile=lib/bundle.esm.js",
    "build:clean": "rimraf ./dist && rimraf ./lib && node ./scripts/prebuild.js && yarn run build",
    "dev": "nodemon",
    "lint": "eslint ./src",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "patch": "npm version patch"
  },
  "publishConfig": {
    "access": "public",
    "branches": [
      "master"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opensource9ja/danfojs.git"
  },
  "keywords": [
    "pandas",
    "data-analysis",
    "data-manipulation",
    "analysis"
  ],
  "author": "Rising Odegua <risingodegua@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opensource9ja/danfojs/issues"
  },
  "homepage": "https://github.com/opensource9ja/danfojs#readme",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.1.2",
    "@types/papaparse": "^5.3.7",
    "@types/plotly.js-dist-min": "^2.3.1",
    "@types/request": "^2.48.8",
    "@types/stream-json": "^1.7.3",
    "@types/table": "^6.3.2",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^9.1.2",
    "browserify": "17.0.0",
    "chai": "^4.3.7",
    "coveralls": "^3.1.1",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.18",
    "eslint": "^8.40.0",
    "ify-loader": "^1.1.0",
    "install-peers": "^1.0.4",
    "json-loader": "^0.5.7",
    "karma": "6.4.2",
    "karma-browserify": "8.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "source-map-loader": "^4.0.1",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "5.82.1",
    "webpack-cli": "5.1.1",
    "yarn": "^1.22.19"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "sideEffects": false
}
